{
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "env": {
    "es2021": true,
    "node": true
  },
  "rules": {
    "no-param-reassign": [2, { "props": false }],
    "max-len": ["error", { "code": 120 }],
    "no-restricted-syntax": "off",
    "indent": [
      "error",
      4,
      { "SwitchCase": 1 }
    ]
  }
}
